<?xml version="1.0" encoding="UTF-8" ?>
<FindBugsFilter
    xmlns="https://github.com/spotbugs/filter/3.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd https://github.com/spotbugs/filter/3.0.0 ">
    <Match>
        <Bug pattern="NP_NULL_ON_SOME_PATH" />
        <!-- Ignore. I see no reason to change this. It's odd this flagged the second dereference and not the first. -->
        <Class name="com.microsoft.applicationinsights.agentc.internal.MainEntryPoint" />
        <Or>
            <Method name="premain" />
            <Method name="initLogging" />
            <Method name="getCodelessSdkNamePrefix" />
        </Or>
        <Field name="startupLogger" />
    </Match>
    <Match>
        <!-- Thses are used for development. Ignore everything from here -->
        <Or>
            <Class name="com.microsoft.applicationinsights.agentc.internal.instrumentation.sdk.TelemetryClientClassFileTransformer$TC" />
            <And>
                <Class name="com.microsoft.applicationinsights.agentc.internal.instrumentation.sdk.TelemetryClientClassFileTransformer" />
                <Method name="main" />
            </And>
        </Or>
    </Match>
    <Match>
        <!-- this is a fire and forget thread. we don't need to hold onto the Future return value -->
        <Bug pattern="RV_RETURN_VALUE_IGNORED_BAD_PRACTICE" />
        <Class name="com.microsoft.applicationinsights.agentc.internal.diagnostics.status.StatusFile" />
        <Method name="write" />
    </Match>
    <Match>
        <Bug pattern="PATH_TRAVERSAL_IN" />
        <Or>
            <And>
                <!-- False Positive. Path is not from an external source. -->
                <Class name="com.microsoft.applicationinsights.agentc.internal.Premain"/>
                <Method name="getAgentJarFile"/>
            </And>
            <And>
                <!-- False Positive. Path is not from an external source. -->
                <Class name="com.microsoft.applicationinsights.agentc.internal.diagnostics.status.StatusFile$1" />
                <Method name="run" />
            </And>
        </Or>
    </Match>
    <Match>
        <Bug pattern="MS_SHOULD_BE_FINAL" />
        <Class name="com.microsoft.applicationinsights.agentc.internal.instrumentation.sdk.UnshadedSdkPackageName" />
        <Field name="ALMOST_PREFIX" />
    </Match>
    <Match>
        <!-- The exception in question comes from Class.forName. It may throw with different versions of java. -->
        <Bug pattern="REC_CATCH_EXCEPTION" />
        <Class name="com.microsoft.applicationinsights.agentc.internal.diagnostics.PidFinder" />
        <Method name="getPidUsingProcessHandle" />
    </Match>
</FindBugsFilter>
